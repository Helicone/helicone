  "module": "dist/semiver.mjs",
  "unpkg": "dist/semiver.min.js",
  "main": "dist/semiver.js",
  "types": "semiver.d.ts",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "lukeed.com"
  },
  "engines": {
