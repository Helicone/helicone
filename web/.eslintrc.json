{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "ignorePatterns": ["lib/clients/jawnTypes/*.ts"],
  "plugins": ["unused-imports", "custom-rules"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-console": "warn",
    "custom-rules/no-next-link": "error",
    "custom-rules/no-next-router": "error"
  }
}
