{
  "name": "socket.io-adapter",
  "version": "2.5.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/socketio/socket.io-adapter.git"
  },
  "files": [
    "dist/"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "default socket.io in-memory adapter",
