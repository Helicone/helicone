{
  "name": "@mintlify/validation",
  "version": "0.1.131",
  "description": "Validates mint.json files",
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "validation"
  ],
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "test:type": "vitest typecheck --run",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@mintlify/models": "0.0.74",
    "lcm": "^0.0.3",
    "lodash": "^4.17.21",
    "openapi-types": "12.x",
    "zod": "^3.20.6",
    "zod-to-json-schema": "^3.20.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@mintlify/eslint-config": "1.0.5",
    "@mintlify/eslint-config-typescript": "1.0.10",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/lcm": "^0.0.0",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "9.x",
    "eslint-plugin-unused-imports": "3.x",
    "openapi-types": "^12.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "ts-expect": "^1.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "gitHead": "28764f2a80a084cd907dd98f37d28afcbd0c6ddc"
}
