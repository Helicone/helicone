  "version": "0.2.0",
  "configurations": [
    {
      "name": "jawn",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}/valhalla/jawn",
      "program": "${workspaceFolder}/valhalla/jawn/src/index.ts",
      "request": "launch",

      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
